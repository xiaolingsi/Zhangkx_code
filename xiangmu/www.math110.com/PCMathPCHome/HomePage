    <script>
        function LoginFun() {
            var Account = $("#Account").val();
            var PassWord = $("#PassWord").val();
            $.ajax({
                type: 'post',
                url: 'LoginFun',
                data: { "PassWord": PassWord, "Account": Account },
                dataType: 'json',
                success: function (result) {
                    if (result.Code == "1") {
                        //Teaching/MathHome/Index
                        window.location.href = "/PCMath/PCHome/HomePage"
                    }
                    else {
                        alert("账号密码不正确")
                    }
                }
            })
        }
    </script>
